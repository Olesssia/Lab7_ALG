def wyszukaj_na_liscie(lista, wartosc):
    if wartosc in lista:
        return True
    else:
        return False


moja_lista = [1, 5, 2, 3, 4, 6, 0, 9]
szukana_wartosc = 3

if wyszukaj_na_liscie(moja_lista, szukana_wartosc):
    print("Wartość została znaleziona na liście.")
else:
    print("Wartość nie została znaleziona na liście.")
